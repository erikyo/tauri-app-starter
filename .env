DB_HOST="127.0.0.1"
DB_PORT="3306"
DB_USERNAME="root"
DB_PASSWORD="pass"
DB_NAME="database"

# This is usually to be set to the same as the SUPERTOKENS_WEBSITE_DOMAIN
CORS_ORIGIN_URL="http://127.0.0.1:3000"

# Api Host Data
# When using Docker, HTTP_HOST will be set automatically to 0.0.0.0
HTTP_HOST="127.0.0.1"
HTTP_PORT="3001"

# Supertokens configuration starts here
# everything can be grabbed from the supertokens dashboard/settings
# Core
SUPERTOKENS_APPNAME="Todo App"
SUPERTOKENS_CONNECTION_URI="http://127.0.0.1:3567"

# Api
SUPERTOKENS_API_DOMAIN="http://127.0.0.1:3001"
SUPERTOKENS_API_KEY="THE_SUPER_SECRET_API_KEY"
SUPERTOKENS_API_BASE_PATH="/auth"

# App
SUPERTOKENS_WEBSITE_DOMAIN="http://localhost:3000"
SUPERTOKENS_WEBSITE_BASE_PATH="/auth"

