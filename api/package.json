{
  "name": "api",
  "version": "1.0.0",
  "description": "api",
  "scripts": {
    "start": "set NODE_ENV=development && nodemon -e ts -x npm run watch:serve",
    "start:debug": "set DEBUG=com.supertokens* && npm run start",
    "watch:serve": "node --loader ts-node/esm src/server.ts ",
    "build": "tsc src/server.ts --outDir lib --module node16",
    "serve": "node lib/server.js",
    "doc": "npm run prepare",
    "test": "jest --coverage"
  },
  "type": "module",
  "main": "./lib/server.js",
  "dependencies": {
    "@fastify/cors": "^8.4.2",
    "@fastify/env": "^4.3.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/mysql": "^4.2.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.1",
    "fastify-plugin": "^4.5.1",
    "fastify-tsconfig": "^2.0.0",
    "joi": "^17.12.2",
    "supertokens-node": "^16.6.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.20",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "fastify-tsconfig": "^2.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "private": true
}
